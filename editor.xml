<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_CUSTOM_FOLDING_REGIONS" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="PREVIEW" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="4" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" pattern="\bfixme\b.*" />
    <pattern icon="question" pattern="\bnote\b.*" />
    <filter name="Fixes">
      <pattern index="1" />
    </filter>
    <filter name="Comments">
      <pattern index="2" />
    </filter>
    <filter name="Todos">
      <pattern index="0" />
    </filter>
  </component>
</application>